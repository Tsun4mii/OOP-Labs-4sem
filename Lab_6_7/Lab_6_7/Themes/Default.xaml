<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Lab_6_7.Themes">
    <Style x:Key="GridTopBar" TargetType="Grid">
        <Setter Property="Background" Value="#4d4c7d"/>
    </Style>
    <Style x:Key="GridLeftBar" TargetType="Grid">
        <Setter Property="Background" Value="#363062"/>
    </Style>
    <Style x:Key="GridPrincipalTheme" TargetType="Grid">
        <Setter Property="Background" Value="#827397"/>
    </Style>
    <Style x:Key="CartBackground" TargetType="Grid">
        <Setter Property="Background" Value="#d8b9c3"/>
    </Style>
    <Style x:Key="CartSeparator" TargetType="Rectangle">
        <Setter Property="Fill" Value="#363062"/>
    </Style>
</ResourceDictionary>